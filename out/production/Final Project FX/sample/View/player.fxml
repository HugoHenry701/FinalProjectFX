<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.PlayerController">
   <children>
      <ComboBox fx:id="playerduo" layoutX="416.0" layoutY="21.0" prefHeight="26.0" prefWidth="169.0" promptText="Choose your turn" />
      <TextField fx:id="playerName" layoutX="64.0" layoutY="114.0" prefHeight="26.0" prefWidth="173.0" />
      <Label alignment="CENTER" layoutX="63.0" layoutY="64.0" prefHeight="18.0" prefWidth="172.0" text="Enter your name" textAlignment="JUSTIFY" />
      <Button fx:id="me" layoutX="283.0" layoutY="115.0" mnemonicParsing="false" onAction="#checkPlayer" text="Check" />
      <Button layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#switchToSceneBoxAndPlayer" prefHeight="25.0" prefWidth="71.0" text="&lt;&lt;Back" />
      <StackPane fx:id="layout" accessibleRole="COMBO_BOX" layoutX="66.0" layoutY="166.0" prefHeight="137.0" prefWidth="169.0">
         <children>
            <ComboBox fx:id="color" accessibleRole="IMAGE_VIEW" prefHeight="25.0" prefWidth="169.0" promptText="Choose your color" />
         </children></StackPane>
      <Button layoutX="498.0" layoutY="345.0" mnemonicParsing="false" onAction="#submitPlayer" prefHeight="41.0" prefWidth="93.0" text="Submit" />
      <Button fx:id="me1" layoutX="283.0" layoutY="222.0" mnemonicParsing="false" onAction="#checkPlayer" text="Check" />
   </children>
</AnchorPane>
